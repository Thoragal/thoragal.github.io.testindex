<mvc:View	controllerName="Homepage.Homepage.controller.WikiDetailView"	
			xmlns:core="sap.ui.core" 
			xmlns:mvc="sap.ui.core.mvc" 
			xmlns="sap.m">
	
	<Shell>
		<App>
			<pages>
				<Page	id="idPageWikiDetail"
						title="Title"
						showHeader="false">
					
					<subHeader>
						<core:Fragment fragmentName="Homepage.Homepage.view.fragments.Header" type="XML"/>
					</subHeader>
					
					<content>
						<core:Fragment fragmentName="Homepage.Homepage.view.fragments.WikiDetail" type="XML"/>
					</content>
					
					<footer>
						<core:Fragment fragmentName="Homepage.Homepage.view.fragments.WikiDetailFooter" type="XML"/>
					</footer>
					
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>